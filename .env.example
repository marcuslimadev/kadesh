# Environment variables for Kadesh Frontend

# API Configuration
VITE_API_URL=https://kadesh-backend.onrender.com
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=Kadesh
VITE_APP_DESCRIPTION=Plataforma de freelancers profissionais
VITE_APP_VERSION=2.0.0

# Features
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=false

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,pdf,doc,docx

# Payments (for future implementation)
VITE_MERCADOPAGO_PUBLIC_KEY=your_mercadopago_public_key_here

# Social Auth (for future implementation)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Production settings
VITE_PRODUCTION_URL=https://kadesh-frontend.vercel.app